{
    "version": 2,
    "builds": [
        {
            "src": "src/backend/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "src/backend/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "index.html"
        }
    ]
}